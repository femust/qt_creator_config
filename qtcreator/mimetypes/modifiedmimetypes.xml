<?xml version="1.0" encoding="UTF-8"?>
<mime-info>
    <mime-type type="text/x-c++src" pattern="*.cpp;*.cxx;*.cc;*.C;*.c++;*.cp">
        <match value="-*- C++ -*-" type="string" offset="0:30" priority="30" mask=""/>
    </mime-type>
    <mime-type type="application/xml" pattern="*.xml;*.xbl;*.xsd;*.rng;*.launch;*.test">
        <match value="&lt;?xml" type="string" offset="0:0" priority="40" mask=""/>
        <match value="&lt;!--" type="string" offset="0:0" priority="40" mask=""/>
    </mime-type>
    <mime-type type="text/vnd.nvidia.cuda.csrc" pattern="*.cu"/>
</mime-info>
